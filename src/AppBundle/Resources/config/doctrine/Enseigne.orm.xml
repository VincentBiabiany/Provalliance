<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Enseigne" table="enseigne">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="logo" type="string" column="logo" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="libelleCourt" type="string" column="libelle_court" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>

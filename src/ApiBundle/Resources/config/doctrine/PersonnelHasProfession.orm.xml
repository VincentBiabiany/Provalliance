<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ApiBundle\Entity\PersonnelHasProfession" table="personnel_has_profession">
    <indexes>
      <index name="fk_personnel_has_profession_profession1_idx" columns="profession_id"/>
      <index name="fk_personnel_has_profession_personnel1_idx" columns="personnel_id"/>
      <index name="fk_personnel_has_profession_date_profession1_idx" columns="date_profession_id"/>
    </indexes>
    <id name="dateProfession" association-key="true"/>
    <id name="personnel" association-key="true"/>
    <id name="profession" association-key="true"/>
    <one-to-one field="dateProfession" target-entity="Date" fetch="LAZY">
      <join-columns>
        <join-column name="date_profession_id" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="personnel" target-entity="Personnel" fetch="LAZY">
      <join-columns>
        <join-column name="personnel_id" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="profession" target-entity="Profession" fetch="LAZY">
      <join-columns>
        <join-column name="profession_id" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>

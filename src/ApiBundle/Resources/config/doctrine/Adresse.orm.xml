<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ApiBundle\Entity\Adresse" table="adresse">
    <indexes>
      <index name="fk_adresse_pays1_idx" columns="pays_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="rue" type="string" column="rue" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cp" type="string" column="cp" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ville" type="string" column="ville" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telephone1" type="string" column="telephone1" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telephone2" type="string" column="telephone2" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fax" type="string" column="fax" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="portable" type="string" column="portable" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="pays" target-entity="Pays" fetch="LAZY">
      <join-columns>
        <join-column name="pays_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ApiBundle\Entity\Salon" table="salon">
    <indexes>
      <index name="fk_salon_adresse1_idx" columns="adresse_id"/>
      <index name="fk_salon_groupe1_idx" columns="groupe_id"/>
      <index name="fk_salon_enseigne1_idx" columns="enseigne_id"/>
      <index name="fk_salon_date1_idx" columns="date_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="codeInterne" type="string" column="code_interne" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="siret" type="string" column="siret" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="marque" type="string" column="marque" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="langueDefaut" type="string" column="langue_defaut" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="marlix" type="string" column="marlix" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="sagePaie" type="integer" column="sage_paie" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="adresse" target-entity="Adresse" fetch="LAZY">
      <join-columns>
        <join-column name="adresse_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="date" target-entity="Date" fetch="LAZY">
      <join-columns>
        <join-column name="date_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="enseigne" target-entity="Enseigne" fetch="LAZY">
      <join-columns>
        <join-column name="enseigne_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="groupe" target-entity="Groupe" fetch="LAZY">
      <join-columns>
        <join-column name="groupe_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="personnel" target-entity="Personnel" mapped-by="salon" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
